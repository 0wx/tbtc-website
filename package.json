{
    "name": "tbtc-website",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "classnames": "^2.2.6",
        "history": "^4.9.0",
        "less": "^3.9.0",
        "less-plugin-clean-css": "^1.5.1",
        "less-watch-compiler": "^1.10.0",
        "node-sass": "^4.13.1",
        "npm-run-all": "^4.1.2",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-router-dom": "^5.1.2",
        "react-scripts": "^3.4.0",
        "react-snap": "^1.23.0"
    },
    "scripts": {
        "env": "truffle exec ./scripts/env.js",
        "start-js": "sleep 2 && react-scripts start",
        "start": "npm-run-all -p start-js",
        "build": "react-scripts --max_old_space_size=8192 build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "publish": "REACT_APP_STATIC=true npm run build"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
